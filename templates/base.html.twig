<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`and uncomment the following Encore helpers to start using
    Symfony UX #}
    <script src="https://kit.fontawesome.com/0a4f7fabdd.js" crossorigin="anonymous"></script>
    <!-- Axentix CSS minified version -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/axentix@1.3.3/dist/css/axentix.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/neu-axentix@1.6.1/dist/css/neu-axentix.min.css">

    {% block stylesheets %}
    {#{{ encore_entry_link_tags('app') }}#}
    {% endblock %}

    <!-- Axentix JS minified version -->
    <script src="https://cdn.jsdelivr.net/npm/axentix@1.3.3/dist/js/axentix.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/neu-axentix@1.6.1/dist/js/neu-axentix.min.js"></script>
    {% block javascripts %}
    {#{{ encore_entry_script_tags('app') }}#}
    {% endblock %}
</head>



<body data-axentix>
    <header class="p-3">
        <nav class="navbar  hoverable-3 shadow-4 blue dark-3 ">
            <a href="/" class="navbar-brand viride">un super shop</a>
            <a class="m-2 nav-link blue dark-2 btn rounded-1 " href="/categories">nos categories</a>
            <a class="m-2 nav-link blue dark-2 btn rounded-1 " href="/products">nos produits</a>
            <a class="m-2 nav-link blue dark-2 btn rounded-1 " href="/register">
                <i class="fas fa-sign-in-alt"></i>
            </a>
        </nav>
    </header>


    {% block body %}{% endblock %}

    <footer class="txt-center  p-5 blue dark-3">
        <!-- Grid container -->

        <!-- Copyright -->
        <div class="footer-content">
            Â© 2021 Copyright:
            <a>made by Rwan</a>
        </div>
        <!-- Copyright -->
    </footer>
    <!-- Footer -->

</body>

</html>